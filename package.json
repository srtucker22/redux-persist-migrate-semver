{
  "name": "redux-persist-migrate-semver",
  "version": "4.1.1",
  "description": "migrate your redux state with semver",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --require babel-register && standard",
    "build": "babel src --out-dir lib",
    "build:watch": "npm run build ./src -- -watch",
    "clean": "rimraf lib dist",
    "postinstall": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srtucker22/redux-persist-migrate-semver.git"
  },
  "homepage": "https://github.com/srtucker22/redux-persist-migrate-semver",
  "author": "rt2zz <zack@root-two.com>",
  "license": "MIT",
  "dependencies": {
    "redux-persist": "^4.4.0",
    "semver": "^5.3.0"
  },
  "keywords": [
    "redux",
    "redux-persist",
    "redux-persist-migrate",
    "redux-migrate"
  ],
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.23.0",
    "mocha": "^3.2.0",
    "redux": "^3.6.0",
    "rimraf": "^2.5.2",
    "standard": "^6.0.8"
  },
  "files": [
    "lib",
    "src"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/lib"
    ],
    "env": [ "mocha" ]
  }
}
